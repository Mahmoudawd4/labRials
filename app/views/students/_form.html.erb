<div class="form-group">
  <%= form.label :name %>
  <%= form.text_field :name, class: "form-control" %>
</div>

<div class="form-group">
  <%= form.label :age %>
  <%= form.number_field :age, class: "form-control" %>
</div>

<div class="form-group">
  <%= form.label :IDno %>
  <%= form.text_field :IDno, class: "form-control" %>
</div>

<div class="form-group">
  <%= form.label :track_id %>
  <%= form.collection_select :track_id, Track.all, :id, :name, {}, class: "form-control" %>
</div>

<div class="actions">
  <%= form.submit class: "btn btn-primary" %>
</div>
